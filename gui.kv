<Dashboard>
	GridLayout:
		rows: 2
		size: root.size

		row_force_default: True
		row_default_height: 160

		GridLayout:
			cols: 6

			# SETTINGS ROW 1
			Label
				text: "BOTH HERE AND \nSOMEWHERE ELSE"

			Label
				text: root.current_time

			Label
				text: ""

			Button:
				text: "SHUFFLE"
				on_press: root.shuffle(0)

			GridLayout:
	            cols: 2
	            Button:
					id: all_0
	                text: '0'
					group: 'all0'
					state: 'normal'
	                on_press: root.all_streaming_one(0)
				Button:
					id: all_1
	                text: '1'
					group: 'all1'
					state: 'normal'
	                on_press: root.all_streaming_one(1)
				Button:
					id: all_2
	                text: '2'
					group: 'all2'
					state: 'normal'
	                on_press: root.all_streaming_one(2)
				Button:
					id: all_3
	                text: '3'
					group: 'all3'
					state: 'normal'
	                on_press: root.all_streaming_one(3)
				Button:
					id: all_4
	                text: '4'
					group: 'all4'
					state: 'normal'
	                on_press: root.all_streaming_one(4)

			GridLayout:
	            cols: 2
				Button:
					id: reset_all
					text: "ALL"
					background_color: 1.0, 0.0, 0.0, 1.0
					on_press: root.reset_lamp(-1)
				GridLayout:
					cols: 2
					Button:
						text: "0"
						background_color: 1.0, 0.0, 0.0, 1.0
						on_press: root.reset_lamp(0)
					Button:
						text: "1"
						background_color: 1.0, 0.0, 0.0, 1.0
						on_press: root.reset_lamp(1)
					Button:
						text: "2"
						background_color: 1.0, 0.0, 0.0, 1.0
						on_press: root.reset_lamp(2)
					Button:
						text: "3"
						background_color: 1.0, 0.0, 0.0, 1.0
						on_press: root.reset_lamp(3)
					Button:
						text: "4"
						background_color: 1.0, 0.0, 0.0, 1.0
						on_press: root.reset_lamp(4)
					Button:
						text: "5"
						background_color: 1.0, 0.0, 0.0, 1.0
						on_press: root.reset_lamp(5)

			# SETTINGS ROW 2

			GridLayout:
				cols: 2
				id: shuffle
				GridLayout:
					rows: 2
					Label:
			            text: "SHUFFLE"
						font_size: 12
					Label:
				        text: root.current_shuffle
				GridLayout:
					rows: 2
					Button:
						id: shuffle_up
						text: "+"
						on_press: root.set_shuffle += 10
					Button:
						id: shuffle_down
						text: "-"
						on_press: root.set_shuffle -= 10

			GridLayout:
				cols: 2
				id: peak
				GridLayout:
					rows: 2
					Label:
			            text: "PEAK"
						font_size: 12
					Label:
				        text: root.current_peak
				GridLayout:
					rows: 2
					Button:
						id: peak_up
						text: "+"
						on_press: root.set_peak += 0.1
					Button:
						id: peak_down
						text: "-"
						on_press: root.set_peak -= 0.1

			GridLayout:
				cols: 2
				id: turn
				GridLayout:
					rows: 2
					Label:
			            text: "TURN"
						font_size: 12
					Label:
				        text: root.current_turn
				GridLayout:
					rows: 2
					Button:
						id: turn_up
						text: "+"
						on_press: root.set_turn += 1
					Button:
						id: turn_down
						text: "-"
						on_press: root.set_turn -= 1

			GridLayout:
				cols: 2
				id: fade
				GridLayout:
					rows: 2
					Label:
			            text: "FADE"
						font_size: 12
					Label:
				        text: root.current_fade
				GridLayout:
					rows: 2
					Button:
						id: fade_up
						text: "+"
						on_press: root.set_fade += 1
					Button:
						id: fade_down
						text: "-"
						on_press: root.set_fade -= 1

			GridLayout:
				cols: 2
				id: hue
				GridLayout:
					rows: 2
					Label:
			            text: "HUE"
						font_size: 12
					Label:
				        text: root.current_hue
				GridLayout:
					rows: 2
					Button:
						id: hue_up
						text: "+"
						on_press: root.set_hue += 1
					Button:
						id: hue_down
						text: "-"
						on_press: root.set_hue -= 1

			GridLayout:
				cols: 2
				id: saturation
				GridLayout:
					rows: 2
					Label:
			            text: "SAT"
						font_size: 12
					Label:
				        text: root.current_saturation
				GridLayout:
					rows: 2
					Button:
						id: saturation_up
						text: "+"
						on_press: root.set_saturation += 1
					Button:
						id: saturation_down
						text: "-"
						on_press: root.set_saturation -= 1

		#-------------------------------------------------------------------
		#Lamp0
		GridLayout:
			cols: 6

			GridLayout:
				rows: 10
				row_force_default: True
				row_default_height: 40

				Label:
					text: ""

				Label:
					text: "Lamp 0"

				ToggleButton:
					id: broadcast_0
					state: 'normal'
					background_color: [0,1,0.3,1]
					on_press: root.manual_broadcast(0)

				Label:
					id: listen_0_to_0
				ToggleButton
					id: listen_0_to_1
					text: '1'
					group: 'listen0'
					state: 'normal'
					on_press: root.manual_listen(0,1)
				ToggleButton
					id: listen_0_to_2
					text: '2'
					group: 'listen0'
					state: 'normal'
					on_press: root.manual_listen(0,2)
				ToggleButton
					id: listen_0_to_3
					text: '3'
					group: 'listen0'
					state: 'normal'
					on_press: root.manual_listen(0,3)
				ToggleButton
					id: listen_0_to_4
					text: '4'
					group: 'listen0'
					state: 'normal'
					on_press: root.manual_listen(0,4)
				ToggleButton
					id: listen_0_to_5
					text: '5'
					group: 'listen0'
					state: 'normal'
					on_press: root.manual_listen(0,5)

				Label:
					text: root.current_time


